{

  "manifest_version": 2,
  "name": "PC School Web",
  "version": "1.0",

  "description": "Try to learn lessons",

  "icons": {
    "48": "icons/disk.png"
  },

  "content_scripts": [
    {
      "matches": ["*://www.google.com/*", "file://*pcschool_web/*"],
      "css" : ["3rd/opensans.css",
               "3rd/bootstrap.css",
               "css/pcschool.css",
               "css/pcschool_lesson_nav.css",
               "css/pcschool_lesson_main.css"],
      "js": ["3rd/jquery-3.0.0.min.js",
             "js/pcschool_web.js",
             "js/pcschool_extension_inject.js"]
    }
  ],
  "web_accessible_resources": [
      "lessons/*",
      "templates/*",
      "test/*"
  ]

}
